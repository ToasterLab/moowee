{
  "plugins": [
      "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "es6": true,
      "browser": true
  },
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "rules": {
    "semi": [1, "never"],
    "no-prototype-builtins": 0,
    "no-use-before-define": 0,
    "object-curly-newline": [0, {"consistent": true}],
    "radix": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "no-alert": 0,
    "one-var": 0,
    "global-require": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "function-paren-newline": [1, "consistent"],
    "no-param-reassign": [1],
    "consistent-return": 1,

    "import/no-extraneous-dependencies":0,

    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/sort-comp":0,
    "react/prefer-stateless-function": 0,
    "react/no-did-mount-set-state": 0
  }
}